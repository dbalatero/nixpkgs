{
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "breadcrumbs.enabled": false,
  "editor.accessibilitySupport": "off",
  "editor.detectIndentation": false,
  "editor.fontFamily": "'JetBrainsMono'",
  "editor.formatOnSave": true,
  "editor.minimap.enabled": false,
  "editor.tabSize": 2,
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "security.workspace.trust.enabled": false,
  "terminal.external.linuxExec": "xterm-256color",
  "terminal.integrated.fontFamily": "'JetBrainsMono'",
  "terminal.integrated.fontSize": 14,
  "update.channel": "none",
  "update.mode": "none",
  "vscode-neovim.neovimClean": false,
  "vscode-neovim.neovimExecutablePaths.darwin": "/Users/db/.nix-profile/bin/nvim",
  "vscode-neovim.neovimInitVimPaths.darwin": "/Users/db/.config/nixpkgs/home/modules/gui/apps/cursor/init.lua",
  "vscode-neovim.compositeKeys": {
    "jk": {
      "command": "vscode-neovim.escape"
    }
  },
  "window.nativeFullScreen": false,
  "window.newWindowDimensions": "maximized",
  "window.zoomLevel": 3,
  "workbench.colorTheme": "Catppuccin Macchiato",
  "workbench.panel.defaultLocation": "right",
  "workbench.statusBar.visible": true,
  "zenMode.fullScreen": false,
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "window.title": "${rootPath}${separator}${activeEditorMedium}",
  "remote.autoForwardPorts": false,
  "remote.restoreForwardedPorts": false,
  "remote.SSH.enableDynamicForwarding": false,
  "remote.forwardOnOpen": false,
  "remote.otherPortsAttributes": {
    "onAutoForward": "ignore"
  },
  "remote.SSH.enableAgentForwarding": false,
  "remote.SSH.path": "sc-ssh-wrapper",
  "remote.SSH.useLocalServer": true,
  "remote.SSH.connectTimeout": 60,
  "remote.SSH.configFile": "~/.ssh/devboxes",
  "remote.SSH.enableRemoteCommand": true,
  "search.followSymlinks": false,
  "debug.javascript.automaticallyTunnelRemoteServer": false,
  "github.copilot.enable": {
    "*": false,
    "apex": true,
    "erb": true,
    "go": true,
    "java": true,
    "javascript": true,
    "python": true,
    "ruby": true,
    "rust": true,
    "scala": true,
    "shellscript": true,
    "starlark": true,
    "terraform": true,
    "typescript": true,
    "typescriptreact": true
  },
  "gitlens.ai.experimental.generateCommitMessage.enabled": false,
  "ruby.rubocop.hideDisableSuggestions": true,
  "remote.autoForwardPortsSource": "output",
  "vale.doNotShowWarningForFileToBeSavedBeforeLinting": true,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "vscode-neovim.neovimInitVimPaths.win32": "/Users/db/.config/nixpkgs/home/modules/gui/apps/cursor/init.lua",
  "telemetry.telemetryLevel": "off",
  "ruby.rubocop.executePath": "/bin/false",
  "gitlens.ai.enabled": false,
  "window.commandCenter": true,
  "workbench.editor.revealIfOpen": false,
  "workbench.editor.focusRecentEditorAfterClose": false,
  "workbench.editor.enablePreview": false,
  "totalTypeScript.hiddenTips": [
    "ts-object-type",
    "basic-types",
    "optional-object-property"
  ],
  "totalTypeScript.hideBasicTips": true,
  "totalTypeScript.hideAllTips": true
}
